{
    "name": "mr-vaco/nova-blog",
    "description": "Blog manager for Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "blog",
        "posts"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "mr-vaco/nova-gallery": "^1.0",
        "mr-vaco/nova-panels": "^1.0.7",
        "mr-vaco/nova-statuses-manager": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "MrVaco\\NovaBlog\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrVaco\\NovaBlog\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
